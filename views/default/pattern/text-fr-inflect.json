re_vowel = re.compile(r\"a|e|i|o|u\", re.I)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "48", "colour": "orange", "code": "is_vowel = lambda ch: ch in VOWELS\n", "error": "none", "author": "tom@organisms.be"}, {"index": "49", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "50", "colour": "orange", "code": "#### PLURALIZE #####################################################################################\n", "error": "none", "author": "tom@organisms.be"}, {"index": "51", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "52", "colour": "orange", "code": "plural_irregular = {\n", "error": "none", "author": "tom@organisms.be"}, {"index": "53", "colour": "orange", "code": "       \"bleu\": \"bleus\",\n", "error": "none", "author": "tom@organisms.be"}, {"index": "54", "colour": "orange", "code": "       \"pneu\": \"pneus\",\n", "error": "none", "author": "tom@organisms.be"}, {"index": "55", "colour": "orange", "code": "    \"travail\": \"travaux\",\n", "error": "none", "author": "tom@organisms.be"}, {"index": "56", "colour": "orange", "code": "    \"vitrail\": \"vitraux\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "57", "colour": "orange", "code": "}\n", "error": "none", "author": "tom@organisms.be"}, {"index": "58", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "59", "colour": "blue", "code": "def pluralize(word, pos=NOUN, custom={}):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "60", "colour": "orange", "code": "    \"\"\" Returns the plural of a given word.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "61", "colour": "orange", "code": "        The custom dictionary is for user-defined replacements.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "62", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "63", "colour": "orange", "code": "    if word in custom:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "64", "colour": "orange", "code": "        return custom[word]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "65", "colour": "orange", "code": "    w = word.lower()\n", "error": "none", "author": "tom@organisms.be"}, {"index": "66", "colour": "orange", "code": "    if w in plural_irregular:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "67", "colour": "orange", "code": "        return plural_irregular[w]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "68", "colour": "orange", "code": "    if w.endswith((\"ais\", \"ois\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "69", "colour": "orange", "code": "        return w + \"es\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "70", "colour": "orange", "code": "    if w.endswith((\"s\", \"x\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "71", "colour": "orange", "code": "        return w\n", "error": "none", "author": "tom@organisms.be"}, {"index": "72", "colour": "orange", "code": "    if w.endswith(\"al\"):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "73", "colour": "orange", "code": "        return w[:-2] + \"aux\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "74", "colour": "orange", "code": "    if w.endswith((\"au\", \"eu\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "75", "colour": "orange", "code": "        return w + \"x\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "76", "colour": "orange", "code": "    return w + \"s\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "77", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "78", "colour": "orange", "code": "#### SINGULARIZE ###################################################################################\n", "error": "none", "author": "tom@organisms.be"}, {"index": "79", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "80", "colour": "blue", "code": "def singularize(word, pos=NOUN, custom={}):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "81", "colour": "orange", "code": "    if word in custom:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "82", "colour": "orange", "code": "        return custom[word]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "83", "colour": "orange", "code": "    w = word.lower()\n", "error": "none", "author": "tom@organisms.be"}, {"index": "84", "colour": "orange", "code": "    # Common articles, determiners, pronouns:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "85", "colour": "orange", "code": "    if pos in (\"DT\", \"PRP\", \"PRP$\", \"WP\", \"RB\", \"IN\"):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "86", "colour": "orange", "code": "        if w == \"du\" : return \"de\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "87", "colour": "orange", "code": "        if w == \"ces\": return \"ce\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "88", "colour": "orange", "code": "        if w == \"les\": return \"le\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "89", "colour": "orange", "code": "        if w == \"des\": return \"un\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "90", "colour": "orange", "code": "        if w == \"mes\": return \"mon\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "91", "colour": "orange", "code": "        if w == \"ses\": return \"son\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "92", "colour": "orange", "code": "        if w == \"tes\": return \"ton\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "93", "colour": "orange", "code": "        if w == \"nos\": return \"notre\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "94", "colour": "orange", "code": "        if w == \"vos\": return \"votre\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "95", "colour": "orange", "code": "        if w.endswith((\"'\", u\"\u2019\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "96", "colour": "orange", "code": "            return w[:-1] + \"e\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "97", "colour": "orange", "code": "    if w.endswith(\"nnes\"):  # parisiennes => parisien\n", "error": "none", "author": "tom@organisms.be"}, {"index": "98", "colour": "orange", "code": "        return w[:-3]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "99", "colour": "orange", "code": "    if w.endswith(\"ntes\"):  # passantes => passant\n", "error": "none", "author": "tom@organisms.be"}, {"index": "100", "colour": "orange", "code": "        return w[:-2]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "101", "colour": "orange", "code": "    if w.endswith(\"euses\"): # danseuses => danseur\n", "error": "none", "author": "tom@organisms.be"}, {"index": "102", "colour": "orange", "code": "        return w[:-3] + \"r\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "103", "colour": "orange", "code": "    if w.endswith(\"s\"):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "104", "colour": "orange", "code": "        return w[:-1]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "105", "colour": "orange", "code": "    if w.endswith((\"aux\", \"eux\", \"oux\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "106", "colour": "orange", "code": "        return w[:-1]  \n", "error": "none", "author": "tom@organisms.be"}, {"index": "107", "colour": "orange", "code": "    if w.endswith(\"ii\"):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "108", "colour": "orange", "code": "        return w[:-1] + \"o\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "109", "colour": "orange", "code": "    if w.endswith((\"ia\", \"ma\")):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "110", "colour": "orange", "code": "        return w[:-1] + \"um\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "111", "colour": "orange", "code": "    if \"-\" in w:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "112", "colour": "orange", "code": "        return singularize(w.split(\"-\")[0]) + \"-\" + \"-\".join(w.split(\"-\")[1:])\n", "error": "none", "author": "tom@organisms.be"}, {"index": "113", "colour": "orange", "code": "    return w\n", "error": "none", "author": "tom@organisms.be"}, {"index": "114", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "115", "colour": "orange", "code": "#### VERB CONJUGATION ##############################################################################\n", "error": "none", "author": "tom@organisms.be"}, {"index": "116", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "117", "colour": "orange", "code": "verb_inflections = [\n", "error": "none", "author": "tom@organisms.be"}, {"index": "118", "colour": "orange", "code": "    (\"issaient\", \"ir\"   ), (\"eassions\",  \"er\"   ), (\"dissions\", \"dre\" ), (u\"\u00e7assions\", \"cer\"  ),\n", "error": "none", "author": "tom@organisms.be"}, {"index": "119", "colour": "orange", "code": "    ( \"eraient\", \"er\"   ), ( \"assions\",  \"er\"   ), ( \"issions\", \"ir\"  ), (  \"iraient\", \"ir\"   ),\n", "error": "none", "author": "tom@organisms.be"}, {"index": "120", "colour": "orange", "code": "    ( \"isaient\", \"ire\"  ), ( \"geaient\",  \"ger\"  ), ( \"eassent\", \"er\"  ), (  \"geasses\", \"ger\"  ),\n", "error": "none", "author": "tom@organisms.be"}, {"index": "121", "colour": "orange", "code": "    ( \"eassiez\", \"er\"   ), ( \"dissiez\",  \"dre\"  ), ( \"dissent\", \"dre\" ), (  \"