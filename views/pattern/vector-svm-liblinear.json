", "code": "for i, s in enumerate(SOLVER_TYPE): \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "47", "code": "\tif s is not None: exec(\"%s = %d\" % (s , i))\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "48", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "49", "code": "PRINT_STRING_FUN = CFUNCTYPE(None, c_char_p)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "50", "code": "def print_null(s): \n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "51", "code": "\treturn \n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "52", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "53", "code": "def genFields(names, types): \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "54", "code": "\treturn list(zip(names, types))\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "55", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "56", "code": "def fillprototype(f, restype, argtypes): \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "57", "code": "\tf.restype = restype\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "58", "code": "\tf.argtypes = argtypes\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "59", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "60", "code": "class feature_node(Structure):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "W", "line": "61", "code": "\t_names = [\"index\", \"value\"]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "62", "code": "\t_types = [c_int, c_double]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "63", "code": "\t_fields_ = genFields(_names, _types)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "64", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "65", "code": "\tdef __str__(self):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "66", "code": "\t\treturn '%d:%g' % (self.index, self.value)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "67", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "68", "code": "def gen_feature_nodearray(xi, feature_max=None, issparse=True):\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "69", "code": "\tif isinstance(xi, dict):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "70", "code": "\t\tindex_range = xi.keys()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "71", "code": "\telif isinstance(xi, (list, tuple)):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "72", "code": "\t\txi = [0] + xi  # idx should start from 1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "73", "code": "\t\tindex_range = range(1, len(xi))\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "74", "code": "\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "75", "code": "\t\traise TypeError('xi should be a dictionary, list or tuple')\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "76", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "77", "code": "\tif feature_max:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "78", "code": "\t\tassert(isinstance(feature_max, int))\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "79", "code": "\t\tindex_range = filter(lambda j: j <= feature_max, index_range)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "80", "code": "\tif issparse: \n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "81", "code": "\t\tindex_range = filter(lambda j:xi[j] != 0, index_range)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "82", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "83", "code": "\tindex_range = sorted(index_range)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "84", "code": "\tret = (feature_node * (len(index_range)+2))()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "85", "code": "\tret[-1].index = -1 # for bias term\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "86", "code": "\tret[-2].index = -1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "87", "code": "\tfor idx, j in enumerate(index_range):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "88", "code": "\t\tret[idx].index = j\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "89", "code": "\t\tret[idx].value = xi[j]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "90", "code": "\tmax_idx = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "91", "code": "\tif index_range : \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "92", "code": "\t\tmax_idx = index_range[-1]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "93", "code": "\treturn ret, max_idx\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "94", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "95", "code": "class problem(Structure):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "W", "line": "96", "code": "\t_names = [\"l\", \"n\", \"y\", \"x\", \"bias\"]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "97", "code": "\t_types = [c_int, c_int, POINTER(c_double), POINTER(POINTER(feature_node)), c_double]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "98", "code": "\t_fields_ = genFields(_names, _types)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "99", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "100", "code": "\tdef __init__(self, y, x, bias = -1):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "101", "code": "\t\tif len(y) != len(x) :\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "102", "code": "\t\t\traise ValueError(\"len(y) != len(x)\")\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "103", "code": "\t\tself.l = l = len(y)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "104", "code": "\t\tself.bias = -1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "105", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "106", "code": "\t\tmax_idx = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "107", "code": "\t\tx_space = self.x_space = []\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "108", "code": "\t\tfor i, xi in enumerate(x):\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "109", "code": "\t\t\ttmp_xi, tmp_idx = gen_feature_nodearray(xi)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "110", "code": "\t\t\tx_space += [tmp_xi]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "111", "code": "\t\t\tmax_idx = max(max_idx, tmp_idx)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "112", "code": "\t\tself.n = max_idx\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "113", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "114", "code": "\t\tself.y = (c_double * l)()\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "115", "code": "\t\tfor i, yi in enumerate(y): self.y[i] = y[i]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "116", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "117", "code": "\t\tself.x = (POINTER(feature_node) * l)() \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "118", "code": "\t\tfor i, xi in enumerate(self.x_space): self.x[i] = xi\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "119", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "120", "code": "\t\tself.set_bias(bias)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "121", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "122", "code": "\tdef set_bias(self, bias):\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "123", "code": "\t\tif self.bias == bias:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "124", "code": "\t\t\treturn \n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "125", "code": "\t\tif bias >= 0 and self.bias < 0: \n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "126", "code": "\t\t\tself.n += 1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "127", "code": "\t\t\tnode = feature_node(self.n, bias)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "128", "code": "\t\tif bias < 0 and self.bias >= 0: \n", "