{"path": "/home/asumal/git/cs410/pattern/pattern/vector/svm/liblinearutil.py", "lines": [[{"error": "C", "line": "1", "code": "#!/usr/bin/env python\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "2", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "3", "code": "import os, sys\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "4", "code": "sys.path = [os.path.dirname(os.path.abspath(__file__))] + sys.path \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "5", "code": "from liblinear import *\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "6", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "7", "code": "def svm_read_problem(data_file_name):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "8", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "9", "code": "\tsvm_read_problem(data_file_name) -> [y, x]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "10", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "11", "code": "\tRead LIBSVM-format data from data_file_name and return labels y\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "12", "code": "\tand data instances x.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "13", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "14", "code": "\tprob_y = []\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "15", "code": "\tprob_x = []\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "16", "code": "\tfor line in open(data_file_name):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "17", "code": "\t\tline = line.split(None, 1)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "18", "code": "\t\t# In case an instance with all zero features\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "19", "code": "\t\tif len(line) == 1: line += ['']\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "20", "code": "\t\tlabel, features = line\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "21", "code": "\t\txi = {}\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "22", "code": "\t\tfor e in features.split():\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "23", "code": "\t\t\tind, val = e.split(\":\")\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "24", "code": "\t\t\txi[int(ind)] = float(val)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "25", "code": "\t\tprob_y += [float(label)]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "26", "code": "\t\tprob_x += [xi]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "27", "code": "\treturn (prob_y, prob_x)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "28", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "29", "code": "def load_model(model_file_name):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "30", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "31", "code": "\tload_model(model_file_name) -> model\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "32", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "33", "code": "\tLoad a LIBLINEAR model from model_file_name and return.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "34", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "35", "code": "\tmodel = liblinear.load_model(model_file_name.encode())\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "36", "code": "\tif not model:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "37", "code": "\t\tprint(\"can't open model file %s\" % model_file_name)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "38", "code": "\t\treturn None\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "39", "code": "\tmodel = toPyModel(model)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "40", "code": "\treturn model\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "41", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "42", "code": "def save_model(model_file_name, model):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "43", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "44", "code": "\tsave_model(model_file_name, model) -> None\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "45", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "46", "code": "\tSave a LIBLINEAR model to the file model_file_name.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "47", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "48", "code": "\tliblinear.save_model(model_file_name.encode(), model)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "49", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "50", "code": "def evaluations(ty, pv):\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "51", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "52", "code": "\tevaluations(ty, pv) -> (ACC, MSE, SCC)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "53", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "54", "code": "\tCalculate accuracy, mean squared error and squared correlation coefficient\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "55", "code": "\tusing the true values (ty) and predicted values (pv).\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "56", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "57", "code": "\tif len(ty) != len(pv):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "58", "code": "\t\traise ValueError(\"len(ty) must equal to len(pv)\")\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "59", "code": "\ttotal_correct = total_error = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "60", "code": "\tsumv = sumy = sumvv = sumyy = sumvy = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "61", "code": "\tfor v, y in zip(pv, ty):\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "62", "code": "\t\tif y == v:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "63", "code": "\t\t\ttotal_correct += 1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "64", "code": "\t\ttotal_error += (v-y)*(v-y)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "65", "code": "\t\tsumv += v\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "66", "code": "\t\tsumy += y\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "67", "code": "\t\tsumvv += v*v\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "68", "code": "\t\tsumyy += y*y\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "69", "code": "\t\tsumvy += v*y\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "70", "code": "\tl = len(ty)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "71", "code": "\tACC = 100.0*total_correct/l\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "72", "code": "\tMSE = total_error/l\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "73", "code": "\ttry:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "74", "code": "\t\tSCC = ((l*sumvy-sumv*sumy)*(l*sumvy-sumv*sumy))/((l*sumvv-sumv*sumv)*(l*sumyy-sumy*sumy))\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "75", "code": "\texcept:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "76", "code": "\t\tSCC = float('nan')\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "77", "code": "\treturn (ACC, MSE, SCC)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "78", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "79", "code": "def train(arg1, arg2=None, arg3=None):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "W", "line": "80", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "81", "code": "\ttrain(y, x [, options]) -> model | ACC\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "82", "code": "\ttrain(prob [, options]) -> model | ACC\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "83", "code": "\ttrain(prob, param) -> model | ACC\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "84", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "85", "code": "\tTrain a model from data (y, x) or a problem prob using\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "86", "code": "\t'options' or a parameter param.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "87", "code": "\tIf '-v' is specified in 'options' (i.e., cross validation)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "88", "code": "\teither accuracy (ACC) or mean-squared error (MSE) is returned.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "89", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "90", "code": "\toptions:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "91", "code": "\t\t-s type : set type of solver (default 1)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "92", "code": "\t\t  for multi-class classification\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "93", "code": "\t\t\t 0 -- L2-regularized logistic regression (primal)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "94", "code": "\t\t\t 1 -- L2-regularized L2-loss support vector classification (dual)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "95", "code": "\t\t\t 2 -- L2-regularized L2-loss support vector classification (primal)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "96", "code": "\t\t\t 3 -- L2-regularized L1-loss support vector classification (dual)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "97", "code": "\t\t\t 4 -- support vector classification by Crammer and Singer\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "98", "code": "\t\t\t 5 -- L1-regularized L2-loss support vector classification\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "99", "code": "\t\t\t 6 -- L1-regularized logistic regression\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "100", "code": "\t\t\t 7 -- L2-regularized logistic regression (dual)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "101", "code": "\t\t  for regression\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "102", "code": "\t\t\t11 -- L2-regularized L2-loss support vector regression (primal)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "103", "code": "\t\t\t12 -- L2-regularized L2-loss support vector regression (dual)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "104", "code": "\t\t\t13 -- L2-regularized L1-loss support vector regression (dual)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "105", "code": "\t\t-c cost : set the parameter C (default 1)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "106", "code": "\t\t-p epsilon : set the epsilon in loss function of SVR (default 0.1)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "107", "code": "\t\t-e epsilon : set tolerance of termination criterion\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "108", "code": "\t\t\t-s 0 and 2\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "109", "code": "\t\t\t\t|f'(w)|_2 <= eps*min(pos,neg)/l*|f'(w0)|_2,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "110", "code": "\t\t\t\twhere f is the primal function, (default 0.01)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "111", "code": "\t\t\t-s 11\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "112", "code": "\t\t\t\t|f'(w)|_2 <= eps*|f'(w0)|_2 (default 0.001)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "113", "code": "\t\t\t-s 1, 3, 4, and 7\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "114", "code": "\t\t\t\tDual maximal violation <= eps; similar to liblinear (default 0.)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "115", "code": "\t\t\t-s 5 and 6\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "116", "code": "\t\t\t\t|f'(w)|_inf <= eps*min(pos,neg)/l*|f'(w0)|_inf,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "117", "code": "\t\t\t\twhere f is the primal function (default 0.01)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "118", "code": "\t\t\t-s 12 and 13\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "119", "code": "\t\t\t\t|f'(alpha)|_1 <= eps |f'(alpha0)|,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "120", "code": "\t\t\t\twhere f is the dual function (default 0.1)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "121", "code": "\t\t-B bias : if bias >= 0, instance x becomes [x; bias]; if < 0, no bias term added (default -1)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "122", "code": "\t\t-wi weight: weights adjust the parameter C of different classes (see README for details)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "123", "code": "\t\t-v n: n-fold cross validation mode\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "124", "code": "\t    -q : quiet mode (no outputs)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "125", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "126", "code": "\tprob, param = None, None\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "127", "code": "\tif isinstance(arg1, (list, tuple)):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "128", "code": "\t\tassert isinstance(arg2, (list, tuple))\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "129", "code": "\t\ty, x, options = arg1, arg2, arg3\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "130", "code": "\t\tprob = problem(y, x)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "131", "code": "\t\tparam = parameter(options)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "132", "code": "\telif isinstance(arg1, problem):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "133", "code": "\t\tprob = arg1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "134", "code": "\t\tif isinstance(arg2, parameter):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "135", "code": "\t\t\tparam = arg2\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "136", "code": "\t\telse :\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "137", "code": "\t\t\tparam = parameter(arg2)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "138", "code": "\tif prob == None or param == None :\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "139", "code": "\t\traise TypeError(\"Wrong types for the arguments\")\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "140", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "141", "code": "\tprob.set_bias(param.bias)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "142", "code": "\tliblinear.set_print_string_function(param.print_func)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "143", "code": "\terr_msg = liblinear.check_parameter(prob, param)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "144", "code": "\tif err_msg :\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "145", "code": "\t\traise ValueError('Error: %s' % err_msg)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "146", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "147", "code": "\tif param.cross_validation:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "148", "code": "\t\tl, nr_fold = prob.l, param.nr_fold\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "149", "code": "\t\ttarget = (c_double * l)()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "150", "code": "\t\tliblinear.cross_validation(prob, param, nr_fold, target)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "151", "code": "\t\tACC, MSE, SCC = evaluations(prob.y[:l], target[:l])\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "E", "line": "152", "code": "\t\tif param.solver_type in [L2R_L2LOSS_SVR, L2R_L2LOSS_SVR_DUAL, L2R_L1LOSS_SVR_DUAL]:\n", "colour": "yellow", "author": "tom@organisms.be"}, {"error": "C", "line": "153", "code": "\t\t\tprint(\"Cross Validation Mean squared error = %g\" % MSE)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "154", "code": "\t\t\tprint(\"Cross Validation Squared correlation coefficient = %g\" % SCC)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "155", "code": "\t\t\treturn MSE\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "156", "code": "\t\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "157", "code": "\t\t\tprint(\"Cross Validation Accuracy = %g%%\" % ACC)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "158", "code": "\t\t\treturn ACC\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "159", "code": "\telse :\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "160", "code": "\t\tm = liblinear.train(prob, param)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "161", "code": "\t\tm = toPyModel(m)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "162", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "163", "code": "\t\treturn m\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "164", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "165", "code": "def predict(y, x, m, options=\"\"):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "W", "line": "166", "code": "\t\"\"\"\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "167", "code": "\tpredict(y, x, m [, options]) -> (p_labels, p_acc, p_vals)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "168", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "169", "code": "\tPredict data (y, x) with the SVM model m.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "170", "code": "\toptions:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "171", "code": "\t    -b probability_estimates: whether to output probability estimates, 0 or 1 (default 0); currently for logistic regression only\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "172", "code": "\t    -q quiet mode (no outputs)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "173", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "174", "code": "\tThe return tuple contains\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "175", "code": "\tp_labels: a list of predicted labels\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "176", "code": "\tp_acc: a tuple including  accuracy (for classification), mean-squared\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "177", "code": "\t       error, and squared correlation coefficient (for regression).\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "178", "code": "\tp_vals: a list of decision values or probability estimates (if '-b 1'\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "179", "code": "\t        is specified). If k is the number of classes, for decision values,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "180", "code": "\t        each element includes results of predicting k binary-class\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "181", "code": "\t        SVMs. if k = 2 and solver is not MCSVM_CS, only one decision value\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "182", "code": "\t        is returned. For probabilities, each element contains k values\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "183", "code": "\t        indicating the probability that the testing instance is in each class.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "184", "code": "\t        Note that the order of classes here is the same as 'model.label'\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "185", "code": "\t        field in the model structure.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "186", "code": "\t\"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "187", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "188", "code": "\tdef info(s):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "189", "code": "\t\tprint(s)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "190", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "191", "code": "\tpredict_probability = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "192", "code": "\targv = options.split()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "193", "code": "\ti = 0\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "194", "code": "\twhile i < len(argv):\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "195", "code": "\t\tif argv[i] == '-b':\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "196", "code": "\t\t\ti += 1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "197", "code": "\t\t\tpredict_probability = int(argv[i])\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "198", "code": "\t\telif argv[i] == '-q':\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "199", "code": "\t\t\tinfo = print_null\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "200", "code": "\t\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "201", "code": "\t\t\traise ValueError(\"Wrong options\")\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "202", "code": "\t\ti+=1\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "203", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "204", "code": "\tsolver_type = m.param.solver_type\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "205", "code": "\tnr_class = m.get_nr_class()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "206", "code": "\tnr_feature = m.get_nr_feature()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "207", "code": "\tis_prob_model = m.is_probability_model()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "208", "code": "\tbias = m.bias\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "209", "code": "\tif bias >= 0:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "210", "code": "\t\tbiasterm = feature_node(nr_feature+1, bias)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "211", "code": "\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "212", "code": "\t\tbiasterm = feature_node(-1, bias)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "213", "code": "\tpred_labels = []\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "214", "code": "\tpred_values = []\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "215", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "216", "code": "\tif predict_probability:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "217", "code": "\t\tif not is_prob_model:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "218", "code": "\t\t\traise TypeError('probability output is only supported for logistic regression')\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "219", "code": "\t\tprob_estimates = (c_double * nr_class)()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "220", "code": "\t\tfor xi in x:\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "221", "code": "\t\t\txi, idx = gen_feature_nodearray(xi, feature_max=nr_feature)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "222", "code": "\t\t\txi[-2] = biasterm\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "223", "code": "\t\t\tlabel = liblinear.predict_probability(m, xi, prob_estimates)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "224", "code": "\t\t\tvalues = prob_estimates[:nr_class]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "225", "code": "\t\t\tpred_labels += [label]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "226", "code": "\t\t\tpred_values += [values]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "227", "code": "\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "228", "code": "\t\tif nr_class <= 2:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "229", "code": "\t\t\tnr_classifier = 1\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "230", "code": "\t\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "231", "code": "\t\t\tnr_classifier = nr_class\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "232", "code": "\t\tdec_values = (c_double * nr_classifier)()\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "233", "code": "\t\tfor xi in x:\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "234", "code": "\t\t\txi, idx = gen_feature_nodearray(xi, feature_max=nr_feature)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "W", "line": "235", "code": "\t\t\txi[-2] = biasterm\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "236", "code": "\t\t\tlabel = liblinear.predict_values(m, xi, dec_values)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "237", "code": "\t\t\tvalues = dec_values[:nr_classifier]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "238", "code": "\t\t\tpred_labels += [label]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "239", "code": "\t\t\tpred_values += [values]\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "240", "code": "\tif len(y) == 0:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "241", "code": "\t\ty = [0] * len(x)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "C", "line": "242", "code": "\tACC, MSE, SCC = evaluations(y, pred_labels)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "243", "code": "\tl = len(y)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "E", "line": "244", "code": "\tif solver_type in [L2R_L2LOSS_SVR, L2R_L2LOSS_SVR_DUAL, L2R_L1LOSS_SVR_DUAL]:\n", "colour": "yellow", "author": "tom@organisms.be"}, {"error": "W", "line": "245", "code": "\t\tinfo(\"Mean squared error = %g (regression)\" % MSE)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "246", "code": "\t\tinfo(\"Squared correlation coefficient = %g (regression)\" % SCC)\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "247", "code": "\telse:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "W", "line": "248", "code": "\t\tinfo(\"Accuracy = %g%% (%d/%d) (classification)\" % (ACC, int(l*ACC/100), l))\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "249", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "250", "code": "\treturn pred_labels, (ACC, MSE, SCC), pred_values\n", "colour": "purple", "author": "tom@organisms.be"}]]}