ne": "15", "code": "# 93% for singularize()\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "16", "code": "# 80% for Verbs.find_lemma() (mixed regular/irregular)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "17", "code": "# 86% for Verbs.find_lexeme() (mixed regular/irregular)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "18", "code": "# 95% predicative() (measured on Lexique French morphology word forms)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "19", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "20", "code": "import os\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "21", "code": "import sys\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "22", "code": "import re\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "23", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "24", "code": "try:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "25", "code": "    MODULE = os.path.dirname(os.path.realpath(__file__))\n", "colour": "black", "author": "tom@alice-2.local"}, {"error": "W", "line": "26", "code": "except:\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "27", "code": "    MODULE = \"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "28", "code": "    \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "29", "code": "sys.path.insert(0, os.path.join(MODULE, \"..\", \"..\", \"..\", \"..\"))\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "30", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "31", "code": "from pattern.text import Verbs as _Verbs\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "W", "line": "32", "code": "from pattern.text import (\n", "colour": "purple", "author": "tom@organisms.be"}, {"error": "none", "line": "33", "code": "    INFINITIVE, PRESENT, PAST, FUTURE,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "34", "code": "    FIRST, SECOND, THIRD,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "35", "code": "    SINGULAR, PLURAL, SG, PL,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "36", "code": "    INDICATIVE, IMPERATIVE, SUBJUNCTIVE, CONDITIONAL,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "37", "code": "    IMPERFECTIVE, PERFECTIVE, PROGRESSIVE,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "38", "code": "    IMPERFECT, PRETERITE,\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "39", "code": "    PARTICIPLE, GERUND\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "40", "code": ")\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "41", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "42", "code": "sys.path.pop(0)\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "43", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "44", "code": "VERB, NOUN, ADJECTIVE, ADVERB = \"VB\", \"NN\", \"JJ\", \"RB\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "45", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "46", "code": "VOWELS = (\"a\", \"e\", \"i\", \"o\", \"u\")\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "47", "code": "re_vowel = re.compile(r\"a|e|i|o|u\", re.I)\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "48", "code": "is_vowel = lambda ch: ch in VOWELS\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "49", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "50", "code": "#### PLURALIZE #####################################################################################\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "51", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "52", "code": "plural_irregular = {\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "53", "code": "       \"bleu\": \"bleus\",\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "54", "code": "       \"pneu\": \"pneus\",\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "55", "code": "    \"travail\": \"travaux\",\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "56", "code": "    \"vitrail\": \"vitraux\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "57", "code": "}\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "58", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "59", "code": "def pluralize(word, pos=NOUN, custom={}):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "none", "line": "60", "code": "    \"\"\" Returns the plural of a given word.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "61", "code": "        The custom dictionary is for user-defined replacements.\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "62", "code": "    \"\"\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "63", "code": "    if word in custom:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "64", "code": "        return custom[word]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "65", "code": "    w = word.lower()\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "66", "code": "    if w in plural_irregular:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "67", "code": "        return plural_irregular[w]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "68", "code": "    if w.endswith((\"ais\", \"ois\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "69", "code": "        return w + \"es\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "70", "code": "    if w.endswith((\"s\", \"x\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "71", "code": "        return w\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "72", "code": "    if w.endswith(\"al\"):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "73", "code": "        return w[:-2] + \"aux\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "74", "code": "    if w.endswith((\"au\", \"eu\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "75", "code": "        return w + \"x\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "76", "code": "    return w + \"s\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "77", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "78", "code": "#### SINGULARIZE ###################################################################################\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "79", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "R", "line": "80", "code": "def singularize(word, pos=NOUN, custom={}):\n", "colour": "green", "author": "tom@organisms.be"}, {"error": "none", "line": "81", "code": "    if word in custom:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "82", "code": "        return custom[word]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "83", "code": "    w = word.lower()\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "84", "code": "    # Common articles, determiners, pronouns:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "85", "code": "    if pos in (\"DT\", \"PRP\", \"PRP$\", \"WP\", \"RB\", \"IN\"):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "86", "code": "        if w == \"du\" : return \"de\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "87", "code": "        if w == \"ces\": return \"ce\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "88", "code": "        if w == \"les\": return \"le\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "89", "code": "        if w == \"des\": return \"un\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "90", "code": "        if w == \"mes\": return \"mon\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "91", "code": "        if w == \"ses\": return \"son\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "92", "code": "        if w == \"tes\": return \"ton\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "93", "code": "        if w == \"nos\": return \"notre\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "94", "code": "        if w == \"vos\": return \"votre\"\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "95", "code": "        if w.endswith((\"'\", u\"\u2019\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "96", "code": "            return w[:-1] + \"e\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "97", "code": "    if w.endswith(\"nnes\"):  # parisiennes => parisien\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "98", "code": "        return w[:-3]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "99", "code": "    if w.endswith(\"ntes\"):  # passantes => passant\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "100", "code": "        return w[:-2]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "101", "code": "    if w.endswith(\"euses\"): # danseuses => danseur\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "102", "code": "        return w[:-3] + \"r\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "103", "code": "    if w.endswith(\"s\"):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "104", "code": "        return w[:-1]\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "105", "code": "    if w.endswith((\"aux\", \"eux\", \"oux\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "106", "code": "        return w[:-1]  \n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "107", "code": "    if w.endswith(\"ii\"):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "108", "code": "        return w[:-1] + \"o\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "109", "code": "    if w.endswith((\"ia\", \"ma\")):\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "110", "code": "        return w[:-1] + \"um\"\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "111", "code": "    if \"-\" in w:\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "112", "code": "        return singularize(w.split(\"-\")[0]) + \"-\" + \"-\".join(w.split(\"-\")[1:])\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "113", "code": "    return w\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "none", "line": "114", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "115", "code": "#### VERB CONJUGATION ##############################################################################\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "none", "line": "116", "code": "\n", "colour": "black", "author": "tom@organisms.be"}, {"error": "C", "line": "117", "code": "verb_inflections = [\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "118", "code": "    (\"issaient\", \"ir\"   ), (\"eassions\",  \"er\"   ), (\"dissions\", \"dre\" ), (u\"\u00e7assions\", \"cer\"  ),\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "119", "code": "    ( \"eraient\", \"er\"   ), ( \"assions\",  \"er\"   ), ( \"issions\", \"ir\"  ), (  \"iraient\", \"ir\"   ),\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "120", "code": "    ( \"isaient\", \"ire\"  ), ( \"geaient\",  \"ger\"  ), ( \"eassent\", \"er\"  ), (  \"geasses\", \"ger\"  ),\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "121", "code": "    ( \"eassiez\", \"er\"   ), ( \"dissiez\",  \"dre\"  ), ( \"dissent\", \"dre\" ), (  \"endrons\", \"endre\"),\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "122", "code": "    ( \"endriez\", \"endre\"), ( \"endrais\",  \"endre\"), (  \"erions\", \"er\"  ), (   \"assent\", \"er\"   ),\n", "colour": "blue", "author": "tom@organisms.be"}, {"error": "C", "line": "123", "code": "    (  \"assiez\", \"er\" 