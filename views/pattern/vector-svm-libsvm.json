{"path": "/home/asumal/git/cs410/pattern/pattern/vector/svm/libsvm.py", "lines": [{"index": "1", "colour": "orange", "code": "#!/usr/bin/env python\n", "error": "none", "author": "tom@organisms.be"}, {"index": "2", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "3", "colour": "green", "code": "from ctypes import *\n", "error": "W", "author": "tom@organisms.be"}, {"index": "4", "colour": "orange", "code": "from ctypes.util import find_library\n", "error": "none", "author": "tom@organisms.be"}, {"index": "5", "colour": "orange", "code": "from os import path\n", "error": "none", "author": "tom@organisms.be"}, {"index": "6", "colour": "orange", "code": "import sys\n", "error": "none", "author": "tom@organisms.be"}, {"index": "7", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "8", "colour": "orange", "code": "# For unix the prefix 'lib' is not considered.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "9", "colour": "orange", "code": "if find_library('svm'):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "10", "colour": "green", "code": "\tlibsvm = CDLL(find_library('svm'))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "11", "colour": "orange", "code": "elif find_library('libsvm'):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "12", "colour": "green", "code": "\tlibsvm = CDLL(find_library('libsvm'))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "13", "colour": "orange", "code": "else:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "14", "colour": "green", "code": "\tb = False\n", "error": "W", "author": "tom@organisms.be"}, {"index": "15", "colour": "green", "code": "\tfor v in (\"libsvm-3.17\", \"libsvm-3.11\"):  # LIBSVM 3.11-17\n", "error": "W", "author": "tom@organisms.be"}, {"index": "16", "colour": "green", "code": "\t\tfor binary in (\n", "error": "W", "author": "tom@organisms.be"}, {"index": "17", "colour": "orange", "code": "\t\t  # If you have OS X 32-bit, you need a 32-bit Python and libsvm-mac32.so.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "18", "colour": "orange", "code": "\t\t  # If you have OS X 32-bit with 64-bit Python, \n", "error": "none", "author": "tom@organisms.be"}, {"index": "19", "colour": "orange", "code": "\t\t  # it will try to load libsvm-mac64.so which fails since OS X is 32-bit.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "20", "colour": "orange", "code": "\t\t  # It won't load libsvm-mac32.so since Python is 64-bit.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "21", "colour": "orange", "code": "\t\t  \"libsvm-win64.dll\",   # 1) 64-bit Windows\n", "error": "none", "author": "tom@organisms.be"}, {"index": "22", "colour": "orange", "code": "\t\t  \"libsvm-win32.dll\",   # 2) 32-bit Windows\n", "error": "none", "author": "tom@organisms.be"}, {"index": "23", "colour": "orange", "code": "\t\t  \"libsvm-mac32.so\",    # 3) 32-bit Mac OS X\n", "error": "none", "author": "tom@organisms.be"}, {"index": "24", "colour": "orange", "code": "\t\t  \"libsvm-mac64.so\",    # 4) 64-bit Mac OS X\n", "error": "none", "author": "tom@organisms.be"}, {"index": "25", "colour": "orange", "code": "\t\t  \"libsvm-ubuntu64.so\", # 5) 64-bit Linux Ubuntu\n", "error": "none", "author": "tom@organisms.be"}, {"index": "26", "colour": "orange", "code": "\t\t  \"libsvm.so\",          # 6) User-compiled Mac / Linux\n", "error": "none", "author": "tom@organisms.be"}, {"index": "27", "colour": "orange", "code": "\t\t  \"libsvm.dll\"):        # 7) User-compiled Windows\n", "error": "none", "author": "tom@organisms.be"}, {"index": "28", "colour": "green", "code": "\t\t\tif sys.platform.startswith(\"win\") and binary.endswith(\".so\"):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "29", "colour": "green", "code": "\t\t\t\tcontinue\n", "error": "W", "author": "tom@organisms.be"}, {"index": "30", "colour": "green", "code": "\t\t\ttry:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "31", "colour": "green", "code": "\t\t\t\tlibsvm = CDLL(path.join(path.dirname(__file__), v, binary)); b=True; break\n", "error": "W", "author": "tom@organisms.be"}, {"index": "32", "colour": "green", "code": "\t\t\texcept OSError as e:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "33", "colour": "green", "code": "\t\t\t\tcontinue\n", "error": "W", "author": "tom@organisms.be"}, {"index": "34", "colour": "green", "code": "\t\tif b: break\n", "error": "W", "author": "tom@organisms.be"}, {"index": "35", "colour": "green", "code": "\tif not b:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "36", "colour": "green", "code": "\t\traise ImportError(\"can't import libsvm (%sbit-%s)\" % (\n", "error": "W", "author": "tom@organisms.be"}, {"index": "37", "colour": "orange", "code": "\t\t\tsizeof(c_voidp) * 8, \n", "error": "none", "author": "tom@organisms.be"}, {"index": "38", "colour": "orange", "code": "\t\t\tsys.platform\n", "error": "none", "author": "tom@organisms.be"}, {"index": "39", "colour": "orange", "code": "\t\t))\n", "error": "none", "author": "tom@organisms.be"}, {"index": "40", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "41", "colour": "orange", "code": "# Construct constants\n", "error": "none", "author": "tom@organisms.be"}, {"index": "42", "colour": "orange", "code": "SVM_TYPE = ['C_SVC', 'NU_SVC', 'ONE_CLASS', 'EPSILON_SVR', 'NU_SVR' ]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "43", "colour": "orange", "code": "KERNEL_TYPE = ['LINEAR', 'POLY', 'RBF', 'SIGMOID', 'PRECOMPUTED']\n", "error": "none", "author": "tom@organisms.be"}, {"index": "44", "colour": "green", "code": "for i, s in enumerate(SVM_TYPE): exec(\"%s = %d\" % (s , i))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "45", "colour": "green", "code": "for i, s in enumerate(KERNEL_TYPE): exec(\"%s = %d\" % (s , i))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "46", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "47", "colour": "orange", "code": "PRINT_STRING_FUN = CFUNCTYPE(None, c_char_p)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "48", "colour": "green", "code": "def print_null(s): \n", "error": "W", "author": "tom@organisms.be"}, {"index": "49", "colour": "green", "code": "\treturn \n", "error": "W", "author": "tom@organisms.be"}, {"index": "50", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "51", "colour": "orange", "code": "def genFields(names, types): \n", "error": "none", "author": "tom@organisms.be"}, {"index": "52", "colour": "green", "code": "\treturn list(zip(names, types))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "53", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "54", "colour": "orange", "code": "def fillprototype(f, restype, argtypes): \n", "error": "none", "author": "tom@organisms.be"}, {"index": "55", "colour": "green", "code": "\tf.restype = restype\n", "error": "W", "author": "tom@organisms.be"}, {"index": "56", "colour": "green", "code": "\tf.argtypes = argtypes\n", "error": "W", "author": "tom@organisms.be"}, {"index": "57", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "58", "colour": "blue", "code": "class svm_node(Structure):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "59", "colour": "green", "code": "\t_names = [\"index\", \"value\"]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "60", "colour": "green", "code": "\t_types = [c_int, c_double]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "61", "colour": "green", "code": "\t_fields_ = genFields(_names, _types)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "62", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "63", "colour": "green", "code": "\tdef __str__(self):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "64", "colour": "green", "code": "\t\treturn '%d:%g' % (self.index, self.value)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "65", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "66", "colour": "orange", "code": "def gen_svm_nodearray(xi, feature_max=None, isKernel=None):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "67", "colour": "green", "code": "\tif isinstance(xi, dict):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "68", "colour": "green", "code": "\t\tindex_range = xi.keys()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "69", "colour": "green", "code": "\telif isinstance(xi, (list, tuple)):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "70", "colour": "green", "code": "\t\tif not isKernel:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "71", "colour": "green", "code": "\t\t\txi = [0] + xi  # idx should start from 1\n", "error": "W", "author": "tom@organisms.be"}, {"index": "72", "colour": "green", "code": "\t\tindex_range = range(len(xi))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "73", "colour": "green", "code": "\telse:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "74", "colour": "green", "code": "\t\traise TypeError('xi should be a dictionary, list or tuple')\n", "error": "W", "author": "tom@organisms.be"}, {"index": "75", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "76", "colour": "green", "code": "\tif feature_max:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "77", "colour": "green", "code": "\t\tassert(isinstance(feature_max, int))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "78", "colour": "green", "code": "\t\tindex_range = filter(lambda j: j <= feature_max, index_range)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "79", "colour": "green", "code": "\tif not isKernel: \n", "error": "W", "author": "tom@organisms.be"}, {"index": "80", "colour": "green", "code": "\t\tindex_range = filter(lambda j:xi[j] != 0, index_range)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "81", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "82", "colour": "green", "code": "\tindex_range = sorted(index_range)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "83", "colour": "green", "code": "\tret = (svm_node * (len(index_range)+1))()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "84", "colour": "green", "code": "\tret[-1].index = -1\n", "error": "W", "author": "tom@organisms.be"}, {"index": "85", "colour": "green", "code": "\tfor idx, j in enumerate(index_range):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "86", "colour": "green", "code": "\t\tret[idx].index = j\n", "error": "W", "author": "tom@organisms.be"}, {"index": "87", "colour": "green", "code": "\t\tret[idx].value = xi[j]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "88", "colour": "green", "code": "\tmax_idx = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "89", "colour": "green", "code": "\tif index_range: \n", "error": "W", "author": "tom@organisms.be"}, {"index": "90", "colour": "green", "code": "\t\tmax_idx = index_range[-1]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "91", "colour": "green", "code": "\treturn ret, max_idx\n", "error": "W", "author": "tom@organisms.be"}, {"index": "92", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "93", "colour": "blue", "code": "class svm_problem(Structure):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "94", "colour": "green", "code": "\t_names = [\"l\", \"y\", \"x\"]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "95", "colour": "green", "code": "\t_types = [c_int, POINTER(c_double), POINTER(POINTER(svm_node))]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "96", "colour": "green", "code": "\t_fields_ = genFields(_names, _types)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "97", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "98", "colour": "green", "code": "\tdef __init__(self, y, x, isKernel=None):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "99", "colour": "green", "code": "\t\tif len(y) != len(x):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "100", "colour": "green", "code": "\t\t\traise ValueError(\"len(y) != len(x)\")\n", "error": "W", "author": "tom@organisms.be"}, {"index": "101", "colour": "green", "code": "\t\tself.l = l = len(y)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "102", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "103", "colour": "green", "code": "\t\tmax_idx = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "104", "colour": "green", "code": "\t\tx_space = self.x_space = []\n", "error": "W", "author": "tom@organisms.be"}, {"index": "105", "colour": "green", "code": "\t\tfor i, xi in enumerate(x):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "106", "colour": "green", "code": "\t\t\ttmp_xi, tmp_idx = gen_svm_nodearray(xi,isKernel=isKernel)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "107", "colour": "green", "code": "\t\t\tx_space += [tmp_xi]\n", "error": "W", "author": "tom@organisms.be"}, {"index": "108", "colour": "green", "code": "\t\t\tmax_idx = max(max_idx, tmp_idx)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "109", "colour": "green", "code": "\t\tself.n = max_idx\n", "error": "W", "author": "tom@organisms.be"}, {"index": "110", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "111", "colour": "green", "code": "\t\tself.y = (c_double * l)()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "112", "colour": "green", "code": "\t\tfor i, yi in enumerate(y): self.y[i] = yi\n", "error": "W", "author": "tom@organisms.be"}, {"index": "113", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "114", "colour": "green", "code": "\t\tself.x = (POINTER(svm_node) * l)() \n", "error": "W", "author": "tom@organisms.be"}, {"index": "115", "colour": "green", "code": "\t\tfor i, xi in enumerate(self.x_space): self.x[i] = xi\n", "error": "W", "author": "tom@organisms.be"}, {"index": "116", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "117", "colour": "blue", "code": "class svm_parameter(Structure):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "118", "colour": "green", "code": "\t_names = [\"svm_type\", \"kernel_type\", \"degree\", \"gamma\", \"coef0\",\n", "error": "W", "author": "tom@organisms.be"}, {"index": "119", "colour": "orange", "code": "\t\t\t\"cache_size\", \"eps\", \"C\", \"nr_weight\", \"weight_label\", \"weight\", \n", "error": "none", "author": "tom@organisms.be"}, {"index": "120", "colour": "orange", "code": "\t\t\t\"nu\", \"p\", \"shrinking\", \"probability\"]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "121", "colour": "green", "code": "\t_types = [c_int, c_int, c_int, c_double, c_double, \n", "error": "W", "author": "tom@organisms.be"}, {"index": "122", "colour": "orange", "code": "\t\t\tc_double, c_double, c_double, c_int, POINTER(c_int), POINTER(c_double),\n", "error": "none", "author": "tom@organisms.be"}, {"index": "123", "colour": "orange", "code": "\t\t\tc_double, c_double, c_int, c_int]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "124", "colour": "green", "code": "\t_fields_ = genFields(_names, _types)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "125", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "126", "colour": "green", "code": "\tdef __init__(self, options = None):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "127", "colour": "green", "code": "\t\tif options == None:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "128", "colour": "green", "code": "\t\t\toptions = ''\n", "error": "W", "author": "tom@organisms.be"}, {"index": "129", "colour": "green", "code": "\t\tself.parse_options(options)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "130", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "131", "colour": "green", "code": "\tdef __str__(self):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "132", "colour": "green", "code": "\t\ts = ''\n", "error": "W", "author": "tom@organisms.be"}, {"index": "133", "colour": "green", "code": "\t\tattrs = svm_parameter._names + list(self.__dict__.keys())\n", "error": "W", "author": "tom@organisms.be"}, {"index": "134", "colour": "green", "code": "\t\tvalues = map(lambda attr: getattr(self, attr), attrs) \n", "error": "W", "author": "tom@organisms.be"}, {"index": "135", "colour": "green", "code": "\t\tfor attr, val in zip(attrs, values):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "136", "colour": "green", "code": "\t\t\ts += (' %s: %s\\n' % (attr, val))\n", "error": "W", "author": "tom@organisms.be"}, {"index": "137", "colour": "green", "code": "\t\ts = s.strip()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "138", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "139", "colour": "green", "code": "\t\treturn s\n", "error": "W", "author": "tom@organisms.be"}, {"index": "140", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "141", "colour": "green", "code": "\tdef set_to_default_values(self):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "142", "colour": "green", "code": "\t\tself.svm_type = C_SVC;\n", "error": "W", "author": "tom@organisms.be"}, {"index": "143", "colour": "green", "code": "\t\tself.kernel_type = RBF\n", "error": "W", "author": "tom@organisms.be"}, {"index": "144", "colour": "green", "code": "\t\tself.degree = 3\n", "error": "W", "author": "tom@organisms.be"}, {"index": "145", "colour": "green", "code": "\t\tself.gamma = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "146", "colour": "green", "code": "\t\tself.coef0 = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "147", "colour": "green", "code": "\t\tself.nu = 0.5\n", "error": "W", "author": "tom@organisms.be"}, {"index": "148", "colour": "green", "code": "\t\tself.cache_size = 100\n", "error": "W", "author": "tom@organisms.be"}, {"index": "149", "colour": "green", "code": "\t\tself.C = 1\n", "error": "W", "author": "tom@organisms.be"}, {"index": "150", "colour": "green", "code": "\t\tself.eps = 0.001\n", "error": "W", "author": "tom@organisms.be"}, {"index": "151", "colour": "green", "code": "\t\tself.p = 0.1\n", "error": "W", "author": "tom@organisms.be"}, {"index": "152", "colour": "green", "code": "\t\tself.shrinking = 1\n", "error": "W", "author": "tom@organisms.be"}, {"index": "153", "colour": "green", "code": "\t\tself.probability = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "154", "colour": "green", "code": "\t\tself.nr_weight = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "155", "colour": "green", "code": "\t\tself.weight_label = (c_int*0)()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "156", "colour": "green", "code": "\t\tself.weight = (c_double*0)()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "157", "colour": "green", "code": "\t\tself.cross_validation = False\n", "error": "W", "author": "tom@organisms.be"}, {"index": "158", "colour": "green", "code": "\t\tself.nr_fold = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "159", "colour": "green", "code": "\t\tself.print_func = None\n", "error": "W", "author": "tom@organisms.be"}, {"index": "160", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "161", "colour": "blue", "code": "\tdef parse_options(self, options):\n", "error": "R", "author": "tom@organisms.be"}, {"index": "162", "colour": "green", "code": "\t\tif isinstance(options, list):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "163", "colour": "green", "code": "\t\t\targv = options\n", "error": "W", "author": "tom@organisms.be"}, {"index": "164", "colour": "green", "code": "\t\telif isinstance(options, str):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "165", "colour": "green", "code": "\t\t\targv = options.split()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "166", "colour": "green", "code": "\t\telse:\n", "error": "W", "author": "tom@organisms.be"}, {"index": "167", "colour": "green", "code": "\t\t\traise TypeError(\"arg 1 should be a list or a str.\")\n", "error": "W", "author": "tom@organisms.be"}, {"index": "168", "colour": "green", "code": "\t\tself.set_to_default_values()\n", "error": "W", "author": "tom@organisms.be"}, {"index": "169", "colour": "green", "code": "\t\tself.print_func = cast(None, PRINT_STRING_FUN)\n", "error": "W", "author": "tom@organisms.be"}, {"index": "170", "colour": "green", "code": "\t\tweight_label = []\n", "error": "W", "author": "tom@organisms.be"}, {"index": "171", "colour": "green", "code": "\t\tweight = []\n", "error": "W", "author": "tom@organisms.be"}, {"index": "172", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "173", "colour": "green", "code": "\t\ti = 0\n", "error": "W", "author": "tom@organisms.be"}, {"index": "174", "colour": "green", "code": "\t\twhile i < len(argv):\n", "error": "W", "author": "tom@organisms.be"}, {"index": "175", "colour": "green", "code": "\t\t\tif argv[i] == \"-s\":\n", "error": "W", "author": "tom@organisms.be"