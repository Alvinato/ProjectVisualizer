{"path": "/home/asumal/git/cs410/plumbum/plumbum/cli/switches.py", "lines": [[{"index": "1", "colour": "orange", "code": "import inspect\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "2", "colour": "orange", "code": "from plumbum.lib import six\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "3", "colour": "orange", "code": "from plumbum import local\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "4", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "5", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "6", "colour": "orange", "code": "class SwitchError(Exception):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "7", "colour": "orange", "code": "    \"\"\"A general switch related-error (base class of all other switch errors)\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "8", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "9", "colour": "orange", "code": "class PositionalArgumentsError(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "10", "colour": "orange", "code": "    \"\"\"Raised when an invalid number of positional arguments has been given\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "11", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "12", "colour": "orange", "code": "class SwitchCombinationError(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "13", "colour": "orange", "code": "    \"\"\"Raised when an invalid combination of switches has been given\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "14", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "15", "colour": "orange", "code": "class UnknownSwitch(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "16", "colour": "orange", "code": "    \"\"\"Raised when an unrecognized switch has been given\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "17", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "18", "colour": "orange", "code": "class MissingArgument(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "19", "colour": "orange", "code": "    \"\"\"Raised when a switch requires an argument, but one was not provided\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "20", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "21", "colour": "orange", "code": "class MissingMandatorySwitch(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "22", "colour": "orange", "code": "    \"\"\"Raised when a mandatory switch has not been given\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "23", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "24", "colour": "orange", "code": "class WrongArgumentType(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "25", "colour": "orange", "code": "    \"\"\"Raised when a switch expected an argument of some type, but an argument of a wrong\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "26", "colour": "orange", "code": "    type has been given\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "27", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "28", "colour": "orange", "code": "class SubcommandError(SwitchError):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "29", "colour": "orange", "code": "    \"\"\"Raised when there's something wrong with subcommands\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "30", "colour": "orange", "code": "    pass\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "31", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "32", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "33", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "34", "colour": "orange", "code": "# The switch decorator\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "35", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "36", "colour": "blue", "code": "class SwitchInfo(object):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "37", "colour": "orange", "code": "    def __init__(self, **kwargs):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "38", "colour": "orange", "code": "        for k, v in kwargs.items():\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "39", "colour": "orange", "code": "            setattr(self, k, v)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "40", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "41", "colour": "blue", "code": "def switch(names, argtype = None, argname = None, list = False, mandatory = False, requires = (),\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "42", "colour": "green", "code": "        excludes = (), help = None, overridable = False, group = \"Switches\"):\n", "error": "W", "author": "Tomer.Filiba@emc.com"}, {"index": "43", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "44", "colour": "orange", "code": "    A decorator that exposes functions as command-line switches. Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "45", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "46", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "47", "colour": "orange", "code": "            @switch([\"-l\", \"--log-to-file\"], argtype = str)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "48", "colour": "orange", "code": "            def log_to_file(self, filename):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "49", "colour": "orange", "code": "                handler = logging.FileHandler(filename)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "50", "colour": "orange", "code": "                logger.addHandler(handler)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "51", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "52", "colour": "orange", "code": "            @switch([\"--verbose\"], excludes=[\"--terse\"], requires=[\"--log-to-file\"])\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "53", "colour": "orange", "code": "            def set_debug(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "54", "colour": "orange", "code": "                logger.setLevel(logging.DEBUG)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "55", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "56", "colour": "orange", "code": "            @switch([\"--terse\"], excludes=[\"--verbose\"], requires=[\"--log-to-file\"])\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "57", "colour": "orange", "code": "            def set_terse(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "58", "colour": "orange", "code": "                logger.setLevel(logging.WARNING)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "59", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "60", "colour": "orange", "code": "    :param names: The name(s) under which the function is reachable; it can be a string\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "61", "colour": "orange", "code": "                  or a list of string, but at least one name is required. There's no need\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "62", "colour": "orange", "code": "                  to prefix the name with ``-`` or ``--`` (this is added automatically),\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "63", "colour": "orange", "code": "                  but it can be used for clarity. Single-letter names are prefixed by ``-``,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "64", "colour": "orange", "code": "                  while longer names are prefixed by ``--``\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "65", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "66", "colour": "orange", "code": "    :param argtype: If this function takes an argument, you need to specify its type. The\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "67", "colour": "orange", "code": "                    default is ``None``, which means the function takes no argument. The type\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "68", "colour": "orange", "code": "                    is more of a \"validator\" than a real type; it can be any callable object\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "69", "colour": "orange", "code": "                    that raises a ``TypeError`` if the argument is invalid, or returns an\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "70", "colour": "orange", "code": "                    appropriate value on success. If the user provides an invalid value,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "71", "colour": "orange", "code": "                    :func:`plumbum.cli.WrongArgumentType`\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "72", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "73", "colour": "orange", "code": "    :param argname: The name of the argument; if ``None``, the name will be inferred from the\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "74", "colour": "orange", "code": "                    function's signature\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "75", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "76", "colour": "orange", "code": "    :param list: Whether or not this switch can be repeated (e.g. ``gcc -I/lib -I/usr/lib``).\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "77", "colour": "orange", "code": "                 If ``False``, only a single occurrence of the switch is allowed; if ``True``,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "78", "colour": "orange", "code": "                 it may be repeated indefinitely. The occurrences are collected into a list,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "79", "colour": "orange", "code": "                 so the function is only called once with the collections. For instance,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "80", "colour": "orange", "code": "                 for ``gcc -I/lib -I/usr/lib``, the function will be called with\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "81", "colour": "orange", "code": "                 ``[\"/lib\", \"/usr/lib\"]``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "82", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "83", "colour": "orange", "code": "    :param mandatory: Whether or not this switch is mandatory; if a mandatory switch is not\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "84", "colour": "orange", "code": "                      given, :class:`MissingMandatorySwitch <plumbum.cli.MissingMandatorySwitch>`\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "85", "colour": "orange", "code": "                      is raised. The default is ``False``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "86", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "87", "colour": "orange", "code": "    :param requires: A list of switches that this switch depends on (\"requires\"). This means that\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "88", "colour": "orange", "code": "                     it's invalid to invoke this switch without also invoking the required ones.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "89", "colour": "orange", "code": "                     In the example above, it's illegal to pass ``--verbose`` or ``--terse``\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "90", "colour": "orange", "code": "                     without also passing ``--log-to-file``. By default, this list is empty,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "91", "colour": "orange", "code": "                     which means the switch has no prerequisites. If an invalid combination\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "92", "colour": "orange", "code": "                     is given, :class:`SwitchCombinationError <plumbum.cli.SwitchCombinationError>`\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "93", "colour": "orange", "code": "                     is raised.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "94", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "95", "colour": "orange", "code": "                     Note that this list is made of the switch *names*; if a switch has more\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "96", "colour": "orange", "code": "                     than a single name, any of its names will do.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "97", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "98", "colour": "orange", "code": "                     .. note::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "99", "colour": "orange", "code": "                        There is no guarantee on the (topological) order in which the actual\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "100", "colour": "orange", "code": "                        switch functions will be invoked, as the dependency graph might contain\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "101", "colour": "orange", "code": "                        cycles.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "102", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "103", "colour": "orange", "code": "    :param excludes: A list of switches that this switch forbids (\"excludes\"). This means that\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "104", "colour": "orange", "code": "                     it's invalid to invoke this switch if any of the excluded ones are given.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "105", "colour": "orange", "code": "                     In the example above, it's illegal to pass ``--verbose`` along with\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "106", "colour": "orange", "code": "                     ``--terse``, as it will result in a contradiction. By default, this list\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "107", "colour": "orange", "code": "                     is empty, which means the switch has no prerequisites. If an invalid\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "108", "colour": "orange", "code": "                     combination is given, :class:`SwitchCombinationError\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "109", "colour": "orange", "code": "                     <plumbum.cli.SwitchCombinationError>` is raised.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "110", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "111", "colour": "orange", "code": "                     Note that this list is made of the switch *names*; if a switch has more\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "112", "colour": "orange", "code": "                     than a single name, any of its names will do.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "113", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "114", "colour": "orange", "code": "    :param help: The help message (description) for this switch; this description is used when\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "115", "colour": "orange", "code": "                 ``--help`` is given. If ``None``, the function's docstring will be used.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "116", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "117", "colour": "orange", "code": "    :param overridable: Whether or not the names of this switch are overridable by other switches.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "118", "colour": "orange", "code": "                        If ``False`` (the default), having another switch function with the same\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "119", "colour": "orange", "code": "                        name(s) will cause an exception. If ``True``, this is silently ignored.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "120", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "121", "colour": "orange", "code": "    :param group: The switch's *group*; this is a string that is used to group related switches\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "122", "colour": "orange", "code": "                  together when ``--help`` is given. The default group is ``Switches``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "123", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "124", "colour": "orange", "code": "    :returns: The decorated function (with a ``_switch_info`` attribute)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "125", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "126", "colour": "orange", "code": "    if isinstance(names, six.string_types):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "127", "colour": "orange", "code": "        names = [names]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "128", "colour": "orange", "code": "    names = [n.lstrip(\"-\") for n in names]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "129", "colour": "orange", "code": "    requires = [n.lstrip(\"-\") for n in requires]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "130", "colour": "orange", "code": "    excludes = [n.lstrip(\"-\") for n in excludes]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "131", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "132", "colour": "orange", "code": "    def deco(func):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "133", "colour": "orange", "code": "        if argname is None:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "134", "colour": "orange", "code": "            argspec = inspect.getargspec(func)[0]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "135", "colour": "orange", "code": "            if len(argspec) == 2:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "136", "colour": "orange", "code": "                argname2 = argspec[1]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "137", "colour": "orange", "code": "            else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "138", "colour": "orange", "code": "                argname2 = \"VALUE\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "139", "colour": "orange", "code": "        else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "140", "colour": "orange", "code": "            argname2 = argname\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "141", "colour": "orange", "code": "        help2 = inspect.getdoc(func) if help is None else help\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "142", "colour": "orange", "code": "        if not help2:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "143", "colour": "orange", "code": "            help2 = str(func)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "144", "colour": "green", "code": "        func._switch_info = SwitchInfo(names = names, argtype = argtype, list = list, func = func,\n", "error": "W", "author": "Tomer.Filiba@emc.com"}, {"index": "145", "colour": "orange", "code": "            mandatory = mandatory, overridable = overridable, group = group,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "146", "colour": "orange", "code": "            requires = requires, excludes = excludes, argname = argname2, help = help2)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "147", "colour": "orange", "code": "        return func\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "148", "colour": "orange", "code": "    return deco\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "149", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "150", "colour": "orange", "code": "def autoswitch(*args, **kwargs):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "151", "colour": "orange", "code": "    \"\"\"A decorator that exposes a function as a switch, \"inferring\" the name of the switch\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "152", "colour": "orange", "code": "    from the function's name (converting to lower-case, and replacing underscores with hyphens).\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "153", "colour": "orange", "code": "    The arguments are the same as for :func:`switch <plumbum.cli.switch>`.\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "154", "colour": "orange", "code": "    def deco(func):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "155", "colour": "green", "code": "        return switch(func.__name__.replace(\"_\", \"-\"), *args, **kwargs)(func)\n", "error": "W", "author": "Tomer.Filiba@emc.com"}, {"index": "156", "colour": "orange", "code": "    return deco\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "157", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "158", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "159", "colour": "orange", "code": "# Switch Attributes\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "160", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "161", "colour": "blue", "code": "class SwitchAttr(object):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "162", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "163", "colour": "orange", "code": "    A switch that stores its result in an attribute (descriptor). Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "164", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "165", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "166", "colour": "orange", "code": "            logfile = SwitchAttr([\"-f\", \"--log-file\"], str)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "167", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "168", "colour": "orange", "code": "            def main(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "169", "colour": "orange", "code": "                if self.logfile:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "170", "colour": "orange", "code": "                    open(self.logfile, \"w\")\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "171", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "172", "colour": "orange", "code": "    :param names: The switch names\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "173", "colour": "orange", "code": "    :param argtype: The switch argument's (and attribute's) type\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "174", "colour": "orange", "code": "    :param default: The attribute's default value (``None``)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "175", "colour": "orange", "code": "    :param argname: The switch argument's name (default is ``\"VALUE\"``)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "176", "colour": "orange", "code": "    :param kwargs: Any of the keyword arguments accepted by :func:`switch <plumbum.cli.switch>`\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "177", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "178", "colour": "orange", "code": "    ATTR_NAME = '__plumbum_switchattr_dict__'\n", "error": "none", "author": "coroa@online.de"}, {"index": "179", "colour": "orange", "code": "\n", "error": "none", "author": "coroa@online.de"}, {"index": "180", "colour": "blue", "code": "    def __init__(self, names, argtype = str, default = None, list = False, argname = \"VALUE\", **kwargs):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "181", "colour": "orange", "code": "        self.__doc__ = \"Sets an attribute\"  # to prevent the help message from showing SwitchAttr's docstring\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "182", "colour": "orange", "code": "        if \"help\" in kwargs and default:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "183", "colour": "orange", "code": "            kwargs[\"help\"] += \"; the default is %r\" % (default,)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "184", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "185", "colour": "orange", "code": "        switch(names, argtype = argtype, argname = argname, list = list, **kwargs)(self)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "186", "colour": "orange", "code": "        listtype = type([])\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "187", "colour": "orange", "code": "        if list:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "188", "colour": "orange", "code": "            if default is None:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "189", "colour": "orange", "code": "                self._default_value = []\n", "error": "none", "author": "coroa@online.de"}, {"index": "190", "colour": "orange", "code": "            elif isinstance(default, (tuple, listtype)):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "191", "colour": "orange", "code": "                self._default_value = listtype(default)\n", "error": "none", "author": "coroa@online.de"}, {"index": "192", "colour": "orange", "code": "            else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "193", "colour": "orange", "code": "                self._default_value = [default]\n", "error": "none", "author": "coroa@online.de"}, {"index": "194", "colour": "orange", "code": "        else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "195", "colour": "orange", "code": "            self._default_value = default\n", "error": "none", "author": "coroa@online.de"}, {"index": "196", "colour": "orange", "code": "\n", "error": "none", "author": "coroa@online.de"}, {"index": "197", "colour": "orange", "code": "    def __call__(self, inst, val):\n", "error": "none", "author": "coroa@online.de"}, {"index": "198", "colour": "orange", "code": "        self.__set__(inst, val)\n", "error": "none", "author": "coroa@online.de"}, {"index": "199", "colour": "orange", "code": "\n", "error": "none", "author": "coroa@online.de"}, {"index": "200", "colour": "orange", "code": "    def __get__(self, inst, cls):\n", "error": "none", "author": "coroa@online.de"}, {"index": "201", "colour": "orange", "code": "        if inst is None:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "202", "colour": "orange", "code": "            return self\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "203", "colour": "orange", "code": "        else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "204", "colour": "orange", "code": "            return getattr(inst, self.ATTR_NAME, {}).get(self, self._default_value)\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "205", "colour": "orange", "code": "\n", "error": "none", "author": "coroa@online.de"}, {"index": "206", "colour": "orange", "code": "    def __set__(self, inst, val):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "207", "colour": "orange", "code": "        if inst is None:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "208", "colour": "orange", "code": "            raise AttributeError(\"cannot set an unbound SwitchAttr\")\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "209", "colour": "orange", "code": "        else:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "210", "colour": "orange", "code": "            if not hasattr(inst, self.ATTR_NAME):\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "211", "colour": "orange", "code": "                setattr(inst, self.ATTR_NAME, {self : val})\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "212", "colour": "orange", "code": "            else:\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "213", "colour": "orange", "code": "                getattr(inst, self.ATTR_NAME)[self] = val\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "214", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "215", "colour": "blue", "code": "class Flag(SwitchAttr):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "216", "colour": "orange", "code": "    \"\"\"A specialized :class:`SwitchAttr <plumbum.cli.SwitchAttr>` for boolean flags. If the flag is not\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "217", "colour": "orange", "code": "    given, the value of this attribute is the ``default``; if it is given, the value changes\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "218", "colour": "orange", "code": "    to ``not default``. Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "219", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "220", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "221", "colour": "orange", "code": "            verbose = Flag([\"-v\", \"--verbose\"], help = \"If given, I'll be very talkative\")\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "222", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "223", "colour": "orange", "code": "    :param names: The switch names\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "224", "colour": "orange", "code": "    :param default: The attribute's initial value (``False`` by default)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "225", "colour": "orange", "code": "    :param kwargs: Any of the keyword arguments accepted by :func:`switch <plumbum.cli.switch>`,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "226", "colour": "orange", "code": "                   except for ``list`` and ``argtype``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "227", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "228", "colour": "orange", "code": "    def __init__(self, names, default = False, **kwargs):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "229", "colour": "orange", "code": "        SwitchAttr.__init__(self, names, argtype = None, default = default, list = False, **kwargs)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "230", "colour": "green", "code": "    def __call__(self, inst):\n", "error": "W", "author": "coroa@online.de"}, {"index": "231", "colour": "orange", "code": "        self.__set__(inst, not self._default_value)\n", "error": "none", "author": "coroa@online.de"}, {"index": "232", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "233", "colour": "blue", "code": "class CountOf(SwitchAttr):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "234", "colour": "orange", "code": "    \"\"\"A specialized :class:`SwitchAttr <plumbum.cli.SwitchAttr>` that counts the number of\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "235", "colour": "orange", "code": "    occurrences of the switch in the command line. Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "236", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "237", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "238", "colour": "orange", "code": "            verbosity = CountOf([\"-v\", \"--verbose\"], help = \"The more, the merrier\")\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "239", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "240", "colour": "orange", "code": "    If ``-v -v -vv`` is given in the command-line, it will result in ``verbosity = 4``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "241", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "242", "colour": "orange", "code": "    :param names: The switch names\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "243", "colour": "orange", "code": "    :param default: The default value (0)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "244", "colour": "orange", "code": "    :param kwargs: Any of the keyword arguments accepted by :func:`switch <plumbum.cli.switch>`,\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "245", "colour": "orange", "code": "                   except for ``list`` and ``argtype``.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "246", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "247", "colour": "orange", "code": "    def __init__(self, names, default = 0, **kwargs):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "248", "colour": "orange", "code": "        SwitchAttr.__init__(self, names, argtype = None, default = default, list = True, **kwargs)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "249", "colour": "orange", "code": "        self._default_value = default  # issue #118\n", "error": "none", "author": "tomerfiliba@gmail.com"}, {"index": "250", "colour": "orange", "code": "    def __call__(self, inst, v):\n", "error": "none", "author": "coroa@online.de"}, {"index": "251", "colour": "orange", "code": "        self.__set__(inst, len(v))\n", "error": "none", "author": "coroa@online.de"}, {"index": "252", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "253", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "254", "colour": "orange", "code": "# Switch type validators\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "255", "colour": "orange", "code": "#===================================================================================================\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "256", "colour": "blue", "code": "class Range(object):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "257", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "258", "colour": "orange", "code": "    A switch-type validator that checks for the inclusion of a value in a certain range.\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "259", "colour": "orange", "code": "    Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "260", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "261", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "262", "colour": "orange", "code": "            age = SwitchAttr([\"--age\"], Range(18, 120))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "263", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "264", "colour": "orange", "code": "    :param start: The minimal value\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "265", "colour": "orange", "code": "    :param end: The maximal value\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "266", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "267", "colour": "orange", "code": "    def __init__(self, start, end):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "268", "colour": "orange", "code": "        self.start = start\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "269", "colour": "orange", "code": "        self.end = end\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "270", "colour": "orange", "code": "    def __repr__(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "271", "colour": "orange", "code": "        return \"[%d..%d]\" % (self.start, self.end)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "272", "colour": "orange", "code": "    def __call__(self, obj):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "273", "colour": "orange", "code": "        obj = int(obj)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "274", "colour": "orange", "code": "        if obj < self.start or obj > self.end:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "275", "colour": "orange", "code": "            raise ValueError(\"Not in range [%d..%d]\" % (self.start, self.end))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "276", "colour": "orange", "code": "        return obj\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "277", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "278", "colour": "blue", "code": "class Set(object):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "279", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "280", "colour": "orange", "code": "    A switch-type validator that checks that the value is contained in a defined\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "281", "colour": "orange", "code": "    set of values. Usage::\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "282", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "283", "colour": "orange", "code": "        class MyApp(Application):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "284", "colour": "orange", "code": "            mode = SwitchAttr([\"--mode\"], Set(\"TCP\", \"UDP\", case_insensitive = False))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "285", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "286", "colour": "orange", "code": "    :param values: The set of values (strings)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "287", "colour": "orange", "code": "    :param case_insensitive: A keyword argument that indicates whether to use case-sensitive\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "288", "colour": "orange", "code": "                             comparison or not. The default is ``True``\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "289", "colour": "orange", "code": "    \"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "290", "colour": "orange", "code": "    def __init__(self, *values, **kwargs):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "291", "colour": "orange", "code": "        self.case_sensitive = kwargs.pop(\"case_sensitive\", False)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "292", "colour": "orange", "code": "        if kwargs:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "293", "colour": "orange", "code": "            raise TypeError(\"got unexpected keyword argument(s): %r\" % (kwargs.keys(),))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "294", "colour": "orange", "code": "        self.values = dict(((v if self.case_sensitive else v.lower()), v) for v in values)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "295", "colour": "orange", "code": "    def __repr__(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "296", "colour": "orange", "code": "        return \"{%s}\" % (\", \".join(repr(v) for v in self.values.values()))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "297", "colour": "orange", "code": "    def __call__(self, obj):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "298", "colour": "orange", "code": "        if not self.case_sensitive:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "299", "colour": "orange", "code": "            obj = obj.lower()\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "300", "colour": "orange", "code": "        if obj not in self.values:\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "301", "colour": "orange", "code": "            raise ValueError(\"Expected one of %r\" % (list(self.values.values()),))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "302", "colour": "orange", "code": "        return self.values[obj]\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "303", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "304", "colour": "blue", "code": "class Predicate(object):\n", "error": "R", "author": "Tomer.Filiba@emc.com"}, {"index": "305", "colour": "orange", "code": "    \"\"\"A wrapper for a single-argument function with pretty printing\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "306", "colour": "orange", "code": "    def __init__(self, func):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "307", "colour": "orange", "code": "        self.func = func\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "308", "colour": "orange", "code": "    def __str__(self):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "309", "colour": "orange", "code": "        return self.func.__name__\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "310", "colour": "orange", "code": "    def __call__(self, val):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "311", "colour": "orange", "code": "        return self.func(val)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "312", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "313", "colour": "orange", "code": "@Predicate\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "314", "colour": "orange", "code": "def ExistingDirectory(val):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "315", "colour": "orange", "code": "    \"\"\"A switch-type validator that ensures that the given argument is an existing directory\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "316", "colour": "orange", "code": "    p = local.path(val)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "317", "colour": "orange", "code": "    if not p.isdir():\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "318", "colour": "orange", "code": "        raise ValueError(\"%r is not a directory\" % (val,))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "319", "colour": "orange", "code": "    return p\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "320", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "321", "colour": "orange", "code": "@Predicate\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "322", "colour": "orange", "code": "def ExistingFile(val):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "323", "colour": "orange", "code": "    \"\"\"A switch-type validator that ensures that the given argument is an existing file\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "324", "colour": "orange", "code": "    p = local.path(val)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "325", "colour": "orange", "code": "    if not p.isfile():\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "326", "colour": "orange", "code": "        raise ValueError(\"%r is not a file\" % (val,))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "327", "colour": "orange", "code": "    return p\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "328", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "329", "colour": "orange", "code": "@Predicate\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "330", "colour": "orange", "code": "def NonexistentPath(val):\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "331", "colour": "orange", "code": "    \"\"\"A switch-type validator that ensures that the given argument is a nonexistent path\"\"\"\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "332", "colour": "orange", "code": "    p = local.path(val)\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "333", "colour": "orange", "code": "    if p.exists():\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "334", "colour": "orange", "code": "        raise ValueError(\"%r already exists\" % (val,))\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "335", "colour": "orange", "code": "    return p\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "336", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "337", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}, {"index": "338", "colour": "orange", "code": "\n", "error": "none", "author": "Tomer.Filiba@emc.com"}]]}