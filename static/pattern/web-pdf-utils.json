n", "error": "none", "author": "tom@organisms.be"}, {"index": "154", "colour": "orange", "code": "    \"\"\"Decodes a PDFDocEncoding string to Unicode.\"\"\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "155", "colour": "orange", "code": "    if s.startswith('\\xfe\\xff'):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "156", "colour": "orange", "code": "        return unicode(s[2:], 'utf-16be', 'ignore')\n", "error": "none", "author": "tom@organisms.be"}, {"index": "157", "colour": "orange", "code": "    else:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "158", "colour": "orange", "code": "        return ''.join( PDFDocEncoding[ord(c)] for c in s )\n", "error": "none", "author": "tom@organisms.be"}, {"index": "159", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "160", "colour": "orange", "code": "# enc\n", "error": "none", "author": "tom@organisms.be"}, {"index": "161", "colour": "orange", "code": "def enc(x, codec='ascii'):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "162", "colour": "orange", "code": "    \"\"\"Encodes a string for SGML/XML/HTML\"\"\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "163", "colour": "orange", "code": "    x = x.replace('&','&amp;').replace('>','&gt;').replace('<','&lt;').replace('\"','&quot;')\n", "error": "none", "author": "tom@organisms.be"}, {"index": "164", "colour": "orange", "code": "    return x.encode(codec, 'xmlcharrefreplace')\n", "error": "none", "author": "tom@organisms.be"}, {"index": "165", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "166", "colour": "orange", "code": "def bbox2str((x0,y0,x1,y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "167", "colour": "orange", "code": "    return '%.3f,%.3f,%.3f,%.3f' % (x0, y0, x1, y1)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "168", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "169", "colour": "orange", "code": "def matrix2str((a,b,c,d,e,f)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "170", "colour": "orange", "code": "    return '[%.2f,%.2f,%.2f,%.2f, (%.2f,%.2f)]' % (a,b,c,d,e,f)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "171", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "172", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "173", "colour": "orange", "code": "##  ObjIdRange\n", "error": "none", "author": "tom@organisms.be"}, {"index": "174", "colour": "orange", "code": "##\n", "error": "none", "author": "tom@organisms.be"}, {"index": "175", "colour": "orange", "code": "class ObjIdRange(object):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "176", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "177", "colour": "orange", "code": "    \"A utility class to represent a range of object IDs.\"\n", "error": "none", "author": "tom@organisms.be"}, {"index": "178", "colour": "orange", "code": "    \n", "error": "none", "author": "tom@organisms.be"}, {"index": "179", "colour": "orange", "code": "    def __init__(self, start, nobjs):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "180", "colour": "orange", "code": "        self.start = start\n", "error": "none", "author": "tom@organisms.be"}, {"index": "181", "colour": "orange", "code": "        self.nobjs = nobjs\n", "error": "none", "author": "tom@organisms.be"}, {"index": "182", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "183", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "184", "colour": "orange", "code": "    def __repr__(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "185", "colour": "orange", "code": "        return '<ObjIdRange: %d-%d>' % (self.get_start_id(), self.get_end_id())\n", "error": "none", "author": "tom@organisms.be"}, {"index": "186", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "187", "colour": "orange", "code": "    def get_start_id(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "188", "colour": "orange", "code": "        return self.start\n", "error": "none", "author": "tom@organisms.be"}, {"index": "189", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "190", "colour": "orange", "code": "    def get_end_id(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "191", "colour": "orange", "code": "        return self.start + self.nobjs - 1\n", "error": "none", "author": "tom@organisms.be"}, {"index": "192", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "193", "colour": "orange", "code": "    def get_nobjs(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "194", "colour": "orange", "code": "        return self.nobjs\n", "error": "none", "author": "tom@organisms.be"}, {"index": "195", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "196", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "197", "colour": "orange", "code": "##  Plane\n", "error": "none", "author": "tom@organisms.be"}, {"index": "198", "colour": "orange", "code": "##\n", "error": "none", "author": "tom@organisms.be"}, {"index": "199", "colour": "orange", "code": "##  A set-like data structure for objects placed on a plane.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "200", "colour": "orange", "code": "##  Can efficiently find objects in a certain rectangular area.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "201", "colour": "orange", "code": "##  It maintains two parallel lists of objects, each of\n", "error": "none", "author": "tom@organisms.be"}, {"index": "202", "colour": "orange", "code": "##  which is sorted by its x or y coordinate.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "203", "colour": "orange", "code": "##\n", "error": "none", "author": "tom@organisms.be"}, {"index": "204", "colour": "orange", "code": "class Plane(object):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "205", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "206", "colour": "orange", "code": "    def __init__(self, objs=None, gridsize=50):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "207", "colour": "orange", "code": "        self._objs = []\n", "error": "none", "author": "tom@organisms.be"}, {"index": "208", "colour": "orange", "code": "        self._grid = {}\n", "error": "none", "author": "tom@organisms.be"}, {"index": "209", "colour": "orange", "code": "        self.gridsize = gridsize\n", "error": "none", "author": "tom@organisms.be"}, {"index": "210", "colour": "orange", "code": "        if objs is not None:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "211", "colour": "orange", "code": "            for obj in objs:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "212", "colour": "orange", "code": "                self.add(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "213", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "214", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "215", "colour": "orange", "code": "    def __repr__(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "216", "colour": "orange", "code": "        return ('<Plane objs=%r>' % list(self))\n", "error": "none", "author": "tom@organisms.be"}, {"index": "217", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "218", "colour": "orange", "code": "    def __iter__(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "219", "colour": "orange", "code": "        return iter(self._objs)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "220", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "221", "colour": "orange", "code": "    def __len__(self):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "222", "colour": "orange", "code": "        return len(self._objs)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "223", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "224", "colour": "orange", "code": "    def __contains__(self, obj):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "225", "colour": "orange", "code": "        return obj in self._objs\n", "error": "none", "author": "tom@organisms.be"}, {"index": "226", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "227", "colour": "orange", "code": "    def _getrange(self, (x0,y0,x1,y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "228", "colour": "orange", "code": "        for y in drange(y0, y1, self.gridsize):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "229", "colour": "orange", "code": "            for x in drange(x0, x1, self.gridsize):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "230", "colour": "orange", "code": "                yield (x,y)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "231", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "232", "colour": "orange", "code": "    \n", "error": "none", "author": "tom@organisms.be"}, {"index": "233", "colour": "orange", "code": "    # add(obj): place an object.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "234", "colour": "orange", "code": "    def add(self, obj):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "235", "colour": "orange", "code": "        for k in self._getrange((obj.x0, obj.y0, obj.x1, obj.y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "236", "colour": "orange", "code": "            if k not in self._grid:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "237", "colour": "orange", "code": "                r = []\n", "error": "none", "author": "tom@organisms.be"}, {"index": "238", "colour": "orange", "code": "                self._grid[k] = r\n", "error": "none", "author": "tom@organisms.be"}, {"index": "239", "colour": "orange", "code": "            else:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "240", "colour": "orange", "code": "                r = self._grid[k]\n", "error": "none", "author": "tom@organisms.be"}, {"index": "241", "colour": "orange", "code": "            r.append(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "242", "colour": "orange", "code": "        self._objs.append(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "243", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "244", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "245", "colour": "orange", "code": "    # remove(obj): displace an object.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "246", "colour": "orange", "code": "    def remove(self, obj):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "247", "colour": "orange", "code": "        for k in self._getrange((obj.x0, obj.y0, obj.x1, obj.y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "248", "colour": "orange", "code": "            try:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "249", "colour": "orange", "code": "                self._grid[k].remove(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "250", "colour": "orange", "code": "            except (KeyError, ValueError):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "251", "colour": "orange", "code": "                pass\n", "error": "none", "author": "tom@organisms.be"}, {"index": "252", "colour": "orange", "code": "        self._objs.remove(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "253", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "254", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "255", "colour": "orange", "code": "    # find(): finds objects that are in a certain area.\n", "error": "none", "author": "tom@organisms.be"}, {"index": "256", "colour": "orange", "code": "    def find(self, (x0,y0,x1,y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "257", "colour": "orange", "code": "        done = set()\n", "error": "none", "author": "tom@organisms.be"}, {"index": "258", "colour": "orange", "code": "        for k in self._getrange((x0,y0,x1,y1)):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "259", "colour": "orange", "code": "            if k not in self._grid: continue\n", "error": "none", "author": "tom@organisms.be"}, {"index": "260", "colour": "orange", "code": "            for obj in self._grid[k]:\n", "error": "none", "author": "tom@organisms.be"}, {"index": "261", "colour": "orange", "code": "                if obj in done: continue\n", "error": "none", "author": "tom@organisms.be"}, {"index": "262", "colour": "orange", "code": "                done.add(obj)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "263", "colour": "orange", "code": "                if (obj.x1 <= x0 or x1 <= obj.x0 or\n", "error": "none", "author": "tom@organisms.be"}, {"index": "264", "colour": "orange", "code": "                    obj.y1 <= y0 or y1 <= obj.y0): continue\n", "error": "none", "author": "tom@organisms.be"}, {"index": "265", "colour": "orange", "code": "                yield obj\n", "error": "none", "author": "tom@organisms.be"}, {"index": "266", "colour": "orange", "code": "        return\n", "error": "none", "author": "tom@organisms.be"}, {"index": "267", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "268", "colour": "orange", "code": "\n", "error": "none", "author": "tom@organisms.be"}, {"index": "269", "colour": "orange", "code": "# create_bmp\n", "error": "none", "author": "tom@organisms.be"}, {"index": "270", "colour": "orange", "code": "def create_bmp(data, bits, width, height):\n", "error": "none", "author": "tom@organisms.be"}, {"index": "271", "colour": "orange", "code": "    info = struct.pack('<IiiHHIIIIII', 40, width, height, 1, bits, 0, len(data), 0, 0, 0, 0)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "272", "colour": "orange", "code": "    assert len(info) == 40, len(info)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "273", "colour": "orange", "code": "    header = struct.pack('<ccIHHI', 'B', 'M', 14+40+len(data), 0, 0, 14+40)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "274", "colour": "orange", "code": "    assert len(header) == 14, len(header)\n", "error": "none", "author": "tom@organisms.be"}, {"index": "275", "colour": "green", "code": "    # XXX re-rasterize every line\n", "error": "W", "author": "tom@organisms.be"}, {"index": "276", "colour": "orange", "code": "    return header+info+data\n", "error": "none", "author": "tom@organisms.be"}]}